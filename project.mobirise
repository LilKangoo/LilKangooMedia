1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\" video-to-image-on-mobile=\"{{videoToImageOnMobile}}\">\n        <div class=\"row\">\n            <div class=\"col-lg-3 col-md-12 image-1\">\n                <img src=\"@PROJECT_PATH@/assets/images/woodstock-2-2048x1638.jpg\" alt=\"Photography in Cyprus\">\n                <img class=\"image-absolute\" src=\"@PROJECT_PATH@/assets/images/lil-red-orange-hat-10-558x558.jpg\" alt=\"Photographer Cyprus\">\n            </div>\n            <div class=\"col-lg-6 col-md-12 text\">\n                <div class=\"text-wrapper\">\n                    <h1 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-1\" mbr-if=\"showTitle\"><b>Lily &amp; Kangoo</b><div><b>Media</b></div></h1>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text\">Photography Videography and Design<br>| Cyprus |</p>\n                </div>\n            </div>\n            <div class=\"col-lg-3 col-md-12 image-2\">\n                <img src=\"@PROJECT_PATH@/assets/images/portrait-4.jpg\" alt=\"Travel in Cyprus\">\n                <img class=\"image-absolute\" src=\"@PROJECT_PATH@/assets/images/scotland-12-1152x2048.jpeg\" alt=\"in Cyprus\">\n            </div>\n        </div>\n    </div>\n</section>",
