              "width": "fit-content",
              "margin": "auto",
              "&:before": {
                "position": "absolute",
                "content": "''",
                "top": "50%",
                "left": "180px",
                "width": "999px",
                "height": "1px",
                "background": "@border"
              },
              "&:after": {
                "position": "absolute",
                "content": "''",
                "top": "50%",
                "right": "180px",
                "width": "999px",
                "height": "1px",
                "background": "@border"
              },
              "position": "relative"
            },
            ".col-lg-12": {
              "overflow": "hidden"
            },
